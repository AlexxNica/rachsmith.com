{
  "firebase": "rachsmith",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "cleanUrls": true,
  "headers": [ {
    "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=3600"
    } ]
  } ]
}